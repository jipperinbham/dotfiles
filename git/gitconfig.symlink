# Local/private config goes in the include
[include]
    path = ~/.gitconfig.local

[commit]
	gpgsign = true

[core]
	compression = 0

[credential]
	helper = osxkeychain

[help]
	autocorrect = 5

[hub]
	protocol = https

[url "https://github.com/"]
	insteadOf = git@github.com:

[url "https://"]
	insteadOf = git://

[pull]
	rebase = true

[gpg]
	program = gpg
